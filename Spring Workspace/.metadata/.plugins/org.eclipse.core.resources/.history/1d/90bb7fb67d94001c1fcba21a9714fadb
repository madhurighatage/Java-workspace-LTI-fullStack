package com.lti.relational.OTO;

import javax.persistence.EntityManager;
import javax.persistence.EntityManagerFactory;
import javax.persistence.Persistence;

public class UserApp 
{
    public static void main( String[] args )
    {
       
       EntityManagerFactory factory = Persistence.createEntityManagerFactory("JPA-PU");
       EntityManager em = factory.createEntityManager();

       Address a2=new Address(1000001L, "Kolhapur", "MH", "India");
       Employee e2=new Employee(102L, "Shravani", 99000, a2);
       
       Address a3=new Address(1000001L, "Pune", "MH", "India");
       Employee e3=new Employee(103L, "Aarthi", 85400, a3);
       Address a4=new Address(1000001L, "Delhi", "DL", "India");
       Employee e4=new Employee(104L, "Seema", 65658, a4);
       Address a5=new Address(1000001L, "Asam", "AS", "India");
       Employee e5=new Employee(105L, "Shweta", 35855, a5);
       
       em.getTransaction().begin();
//       em.find(Employee.class, 101L);
//       em.remove(e1);
       //em.persist(e1);
       em.getTransaction().commit();
       
    }
}
